# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'MusicApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MusicApp
  pod 'IQKeyboardManagerSwift', '~> 6.0'
  pod 'RealmSwift'
  pod 'SnapKit', git: 'https://github.com/SnapKit/SnapKit', branch: 'develop'
  pod 'SwiftRichString', git: 'https://github.com/malcommac/SwiftRichString', branch: 'develop'
  
  target 'MusicAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MusicAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
